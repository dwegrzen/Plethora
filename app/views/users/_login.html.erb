<%= simple_form_for :loginuser do |f| %>
  <%= f.error_notification %>

  <div class="col-sm-6 col-sm-offset-3"  id="loginForm">

    <div class="form-inputs">
      <%= f.input :email %>
      <%= f.input :password %>

    </div>

    <div class="form-actions">
      <%= f.button :submit, 'Login', class: "btn-primary" %>
    </div>
  <% end %>
 </div>
