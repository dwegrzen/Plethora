<h4 id="notice"><span class="label label-info"><%= notice %></span></h4>

<%= form_tag search_path, id: 'searchBarAndButton', method: "get" do %>
<%= text_field_tag :search, params[:search], placeholder: "Show, artist, whatever else", id: 'searchBar' %>

<%= button_tag(type: 'submit', class: 'btn', id: 'searchBtn') do %>
  <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
<% end %>

<% end %>

<div class="container">
  <div class="row">
    <% if !@tvparse %>
      <h4>No TV search results</h4>
    <% end %>
  </div>
  <h3>Artist search results:</h3>
  <!-- <%= @artistparse.to_json %> -->
</div>
<div class="container">
  <div id="results">

  </div>

<script>
  var TVItems = <%== @tvparse.to_json %>;
  console.log(TVItems)
  var userQueuedShows = <%==@userqueuedshows.to_json %>;
  var userFinishedShows = <%==@userfinishedshows.to_json %>;
  var MusicItems = <%== @artistparse.to_json %>;
  console.log(MusicItems)
  var userQueuedMusic = <%==@userqueuedmusic.to_json %>;
  console.log(userQueuedMusic)
  var userFinishedMusic = <%==@userfinishedmusic.to_json %>;
  console.log(userFinishedMusic)
</script>
<script src="/js/results.bundle.js"></script>



